$().ready(function() {
	var elf = $('#elfinder').elfinder({
		height:'700',	
		url : '{{ asset('bundles/admin/bower_components/elFinder-2.0.7/php/connector.minimal.php') }}',  // connector URL (REQUIRED)
		lang: 'fr',				
		resizable:true,
		rememberLastDir :true,
		ui : ['toolbar', 'places', 'tree', 'path', 'stat'],
	}).elfinder('instance');  
	
});
</script>

{% endblock %}
	
